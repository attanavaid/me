{
  "name": "me",
  "homepage": "https://attanavaid.github.io/me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy-redirect": "node deploy-redirect.js && gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "npm run build && source-map-explorer \"build/static/js/*.js\"",
    "analyze-bundle": "npm run build && node -e \"const fs=require('fs');const files=fs.readdirSync('build/static/js').filter(f=>f.endsWith('.js'));console.log('Bundle files:',files.join('\\n'));\"",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,scss,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@formspark/use-formspark": "^0.1.42",
    "@material-ui/core": "^4.12.4",
    "@sendgrid/mail": "^8.1.6",
    "emailjs-com": "^3.2.0",
    "gsap": "^3.14.2",
    "react": "^19.2.3",
    "react-alert": "^7.0.3",
    "react-chrono": "^3.3.3",
    "react-dom": "^19.2.3",
    "react-glitch-effect": "^3.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-typing-animation": "^1.6.2",
    "react-typist": "^2.0.5",
    "reactjs-popup": "^2.0.6",
    "sass": "^1.97.0",
    "swiper": "^12.0.3",
    "web-vitals": "^5.1.0",
    "webpack": "^5.104.0",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "compression-webpack-plugin": "^11.1.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "json-loader": "^0.5.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "react-scripts": "5.0.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.19",
    "terser-webpack-plugin": "^5.3.16",
    "webpack-bundle-analyzer": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
